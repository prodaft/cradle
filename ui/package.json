        "vite": "^5.2.0",
        "vite-plugin-environment": "^1.1.3"
    },
    "build": {
        "appId": "com.prodaft.cradle",
        "productName": "Cradle",
        "files": [
            "out/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "directories": {
            "buildResources": "resources",
            "output": "dist"
        },
        "extraResources": [
            {
                "from": "assets/",
                "to": "assets",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "win": {
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "shortcutName": "Cradle"
        },
        "linux": {
            "target": [
                "AppImage"
            ]
        },
        "appImage": {
            "category": "Utility"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "prodaft",
                "repo": "cradle",
                "releaseType": "release"
            }
        ]
    },
    "msw": {
        "workerDirectory": [
            "./public"
        ]
    }
}
